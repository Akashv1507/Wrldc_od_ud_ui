{# section 1 table consumption(MUs) #}
  <h3>1. Energy Consumption</h3>
  <table id="section1Tbl" class="table table-bordered table-hover display" style="width:100%">
      <thead>
          <tr>
              <th>Date</th>
              {% for rowHeading in section1ConsumpData[0]['values'] %}
              <th>{{rowHeading['STATE_NAME'][:]}}</th>
              {% endfor %}
          </tr>
      </thead>    
      <tbody>
          {% for row in section1ConsumpData %}
          <tr>
              <td>{{row.date}}</td>
              {% for rowval in row['values'] %}
                <td>{{rowval['CONSUMPTION']}}</td>
              {% endfor %}
          </tr>
          {% endfor %}  
          <tr>
              <td> Change</td>
              <td> {{(section1ConsumpData[1]['values'][0]['CONSUMPTION']- section1ConsumpData[0]['values'][0]['CONSUMPTION'])|round(2)}}</td>
              <td> {{(section1ConsumpData[1]['values'][1]['CONSUMPTION']- section1ConsumpData[0]['values'][1]['CONSUMPTION'])|round(2)}}</td>
              <td> {{(section1ConsumpData[1]['values'][2]['CONSUMPTION']- section1ConsumpData[0]['values'][2]['CONSUMPTION'])|round(2)}}</td>
              <td> {{(section1ConsumpData[1]['values'][3]['CONSUMPTION']- section1ConsumpData[0]['values'][3]['CONSUMPTION'])|round(2)}}</td>
              <td> {{(section1ConsumpData[1]['values'][4]['CONSUMPTION']- section1ConsumpData[0]['values'][4]['CONSUMPTION'])|round(2)}}</td>
              <td> {{(section1ConsumpData[1]['values'][5]['CONSUMPTION']- section1ConsumpData[0]['values'][5]['CONSUMPTION'])|round(2)}}</td>
              <td> {{(section1ConsumpData[1]['values'][6]['CONSUMPTION']- section1ConsumpData[0]['values'][6]['CONSUMPTION'])|round(2)}}</td>
              <td> {{(section1ConsumpData[1]['values'][7]['CONSUMPTION']- section1ConsumpData[0]['values'][7]['CONSUMPTION'])|round(2)}}</td>
              <td> {{(section1ConsumpData[1]['values'][8]['CONSUMPTION']- section1ConsumpData[0]['values'][8]['CONSUMPTION'])|round(2)}}</td>
